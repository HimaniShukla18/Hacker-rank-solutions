#include <stdio.h>

int main() {
    int n, m;

    
    scanf("%d", &n);
    int ranked[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &ranked[i]);
    }

    
    scanf("%d", &m);
    int player[m];
    for (int i = 0; i < m; i++) {
        scanf("%d", &player[i]);
    }

    
    int unique_ranks[n];
    int rank_count = 0;
    if (n > 0) {
        unique_ranks[rank_count++] = ranked[0];
        for (int i = 1; i < n; i++) {
            if (ranked[i] != ranked[i - 1]) {
                unique_ranks[rank_count++] = ranked[i];
            }
        }
    }
    
   
    int rank_index = rank_count - 1;

   
    for (int i = 0; i < m; i++) {
        int current_score = player[i];

        
        while (rank_index >= 0 && current_score >= unique_ranks[rank_index]) {
            rank_index--;
        }

       
        printf("%d\n", rank_index + 2);
    }

    return 0;
}
