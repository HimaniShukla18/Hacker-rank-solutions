#include <stdio.h>
#include <stdlib.h>

void cutTheSticks(int arr[], int n) {
    int* sticks = (int*)malloc(n * sizeof(int));
    int count = n;

    // Copy input array to sticks
    for (int i = 0; i < n; i++) {
        sticks[i] = arr[i];
    }

    while (count > 0) {
        printf("%d\n", count);

        // Find the minimum non-zero stick length
        int min = 1000000;
        for (int i = 0; i < n; i++) {
            if (sticks[i] > 0 && sticks[i] < min) {
                min = sticks[i];
            }
        }

        // Cut sticks and count remaining
        int newCount = 0;
        for (int i = 0; i < n; i++) {
            if (sticks[i] > 0) {
                sticks[i] -= min;
                if (sticks[i] > 0) {
                    newCount++;
                }
            }
        }

        count = newCount;
    }

    free(sticks);
}

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    cutTheSticks(arr, n);
    return 0;
}
